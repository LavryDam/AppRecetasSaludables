{
  "expo": {
    "name": "AppRecetasSaludables",
    "slug": "AppRecetasSaludables",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "AppRecetasSaludables",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#E0F8E0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dina.apprecetassaludables"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dina.apprecetassaludables"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c155ac54-da12-4ce3-b951-8744d0165a5d"
      }
    }
  }
}
